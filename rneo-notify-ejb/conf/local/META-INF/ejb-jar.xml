<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
    version="3.2">
    <display-name>Ruslan-Neo Notification module</display-name>
    <enterprise-beans>
    	<message-driven>
    		<ejb-name>Notifier</ejb-name>
            <env-entry>
                <env-entry-name>com.ols.ruslan.neo.jbpm.RecordEventTranslator/ruslanInstanceUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>https://ruslan.library.spbstu.ru/rrs-web/</env-entry-value>
            </env-entry>
    	</message-driven>
    </enterprise-beans>
</ejb-jar>
